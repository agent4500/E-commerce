                                <input type="hidden" name="_csrf" value="<%= csrfToken %>" class="form-control border-2 border-secondary w-75 py-3 px-4 rounded-pill">
                                <button type="submit" class="btn btn-primary border-2 border-secondary py-3 px-5 position-absolute rounded-pill text-white h-100" style="top: 0; right: 25%;">Search</button>
                            </form>
                        </div>
                    </div>
                    <div class="col-md-12 col-lg-5">
